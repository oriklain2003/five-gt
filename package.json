{
  "name": "ground-truth-data-creator",
  "version": "1.0.0",
  "description": "Full-stack web app to create ground truth data for XGBoost model",
  "main": "backend/dist/index.js",
  "scripts": {
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "build-backend": "cd backend && npm run build",
    "build-frontend": "cd frontend && npm run build",
    "build": "npm run install-backend && npm run install-frontend && npm run build-backend && npm run build-frontend",
    "start": "cd backend && npm start",
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\""
  },
  "keywords": [
    "xgboost",
    "ground-truth",
    "data-labeling",
    "machine-learning"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
